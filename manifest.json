{
  "manifest_version": 3,
  "name": "Google Maps Search Opener",
  "version": "1.0",
  "description": "Can't find the maps link? Use this extension to open your search in Google Maps.",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["*://www.google.com/search*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}